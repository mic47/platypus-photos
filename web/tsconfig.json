{
  "compilerOptions": {
    "isolatedModules": true,
    "moduleResolution": "Bundler",
    "module": "es2022",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es2021",
    "strict": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "es2021"
    ]
  },
  "exclude": [
    "platypus-photos-phone"
  ]
}
