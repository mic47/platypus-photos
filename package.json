{
  "name": "speedscope",
  "type": "module",
  "scripts": {
    "prettier": "prettier --write 'ts/**/*.ts'",
    "lint": "eslint 'ts/**/*.ts'",
    "coverage": "npm run jest -- --coverage",
    "typecheck": "tsc --noEmit",
    "dev-watch": "yarn parcel watch typescript/index.html --no-cache --dist-dir \"static/\" --public-url \"http://localhost:8000/static\" --detailed-report",
    "prod-build": "yarn parcel build typescript/index.html --no-cache --dist-dir \"static/\" --public-url \"http://localhost:8000/static\" --detailed-report --no-optimize",
    "__last-one__": "Just to have trailing comma"
  },
  "prettier": {
    "tabWidth": 4
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "devDependencies": {
    "@types/leaflet": "^1.9.12",
    "eslint": "^9.5.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.6.0",
    "leaflet": "^1.9.4"
  }
}
